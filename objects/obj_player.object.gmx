      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Platform physics

var rkey = keyboard_check(ord("D"));
var lkey = keyboard_check(ord("A"));
var jkey = keyboard_check(vk_space);
var dash_key = keyboard_check_pressed(ord("L"));
var up_key = keyboard_check(ord("W"));
var up_dash_key = keyboard_check_pressed(186);

var onGround = place_meeting(x, y+1, obj_solid);

var moveOnGround = spd/3;
var moveOnAir = spd/2;
var frictionOnGround = spd/3;
var dragOnAir = spd/30;
var frictionOnGroundNoKeys = spd/2;
var dragOnAirNoKeys = spd/15;

var wallJumpSpd = spd * 1.7;
var dashSpd = spd * 4;
var upDashSpd = jspd * 1.5;

// Check for ground
if (onGround) {
    vspd = 0;

    // Jumping
    if (jkey) {
         vspd = -jspd;
    }
} else {
    // Gravity
    if (vspd &lt; 10) {
        vspd += grav;
    }
}

// Moving right
if (rkey) {
    facing_right = true;
    if (hspd &lt;= spd) {
        if (onGround) {
            hspd += moveOnGround;
        } else {
            hspd += moveOnAir;
        }
    }
    dash_direction = vk_right;
}

// Moving left
if (lkey) {
    facing_right = false;
    if (hspd &gt;= -spd) {
        if (onGround) {
            hspd -= moveOnGround;
        } else {
            hspd -= moveOnAir;
        }
    }
    dash_direction = vk_left;
}

// Dashing

if (dash_key) {
    if (can_dash &amp;&amp; can_use_skill) {
        can_use_skill = false;
        alarm[2] = skill_cooldown;
        can_dash = false;
        alarm[0] = dash_cooldown;
        if (dash_direction == vk_right) {
            hspd = dashSpd;
        } else if (dash_direction == vk_left) {
            hspd = -dashSpd;
        }
    }
}

// Up dashing

if (up_dash_key &amp;&amp; !place_meeting(x, y+1, obj_solid)) {
    if (can_up_dash &amp;&amp; can_use_skill) {
        can_use_skill = false;
        alarm[2] = skill_cooldown;
        can_up_dash = false;
        alarm[1] = up_dash_cooldown;
        vspd = -upDashSpd;
    }
}

// Wall jumping
if (place_meeting(x+1, y, obj_solid) || place_meeting(x-1, y, obj_solid)) {
    // slow gravity
    if (!onGround) {
        vspd = min(2, vspd);
    }

    if (lkey &amp;&amp; place_meeting(x+1, y, obj_solid)) {
        if (!wall_jump_unstick) {
            hspd = 0;
            if (!wall_jump_unstick_timer_started) {
                alarm[3] = 10;
                wall_jump_unstick_timer_started = true;
            }
        } else {
            wall_jump_unstick = false;
        }
    } else if (rkey &amp;&amp; place_meeting(x-1, y, obj_solid)) {
        if (!wall_jump_unstick) {
            hspd = 0;
            if (!wall_jump_unstick_timer_started) {
                alarm[3] = 10;
                wall_jump_unstick_timer_started = true;
            }
        } else {
            wall_jump_unstick = false;
        }
    } else {
        wall_jump_unstick_timer_started = false;
    }


    if (keyboard_check_pressed(vk_space)) {
        vspd = -jspd;
        if (place_meeting(x+1, y, obj_solid)) {
            hspd = -1 * wallJumpSpd;
        } else if (place_meeting(x-1, y, obj_solid)) {
            hspd = wallJumpSpd;
        }
    }
}


// Horizontal collisions
// If the player is reaching a wall, x+1 until it reaches the edge
if (place_meeting(x+hspd, y, obj_solid)) {
    while(!place_meeting(x+sign(hspd), y, obj_solid)) {
        x += sign(hspd);
    }
    hspd = 0;
}


// Friction, drag, etc.
if (hspd &gt; spd) {
    if (onGround) {
        hspd -= frictionOnGround;
    } else {
        hspd -= dragOnAir
    }
}  else if (hspd &lt; -spd) {
    if (onGround) {
        hspd += frictionOnGround;
    } else {
        hspd += dragOnAir;
    }
}

// Check if player is not moving
if ((!rkey &amp;&amp; !lkey) || (rkey &amp;&amp; lkey)) {
    if (hspd &gt; 0) {
        if (onGround) {
            hspd -= frictionOnGroundNoKeys;
        } else {
            hspd -= dragOnAirNoKeys;
        }
    } else if (hspd &lt; 0) {
        if (onGround) {
            hspd += frictionOnGroundNoKeys;
        } else {
            hspd += dragOnAirNoKeys;
        }
    } else {
        hspd = 0;
    }
    dash_direction = vk_control;

    if (abs(hspd) &lt; 1) {
        hspd = 0;
    }
}

// Move horizontally
x += hspd;

// Vertical collisions
// If the player is reaching a wall, y+1 until it reaches the edge
if (place_meeting(x, y+vspd, obj_solid)) {
    while(!place_meeting(x, y+sign(vspd), obj_solid)) {
        y += sign(vspd);
    }
    vspd = 0;
}

// Move hertically
y += vspd;

show_debug_message(hspd);

</string>
          </argument>
        </arguments>
